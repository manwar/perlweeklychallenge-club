#!/usr/bin/env raku
use Test;

is ufg(3), < 1/3 1/2 2/3 1/1 3/2 2/1 3/1 >;
is ufg(4), < 1/4 1/3 1/2 2/3 3/4 1/1 4/3 3/2 2/1 3/1 4/1 >;
is ufg(1), < 1/1 >;
is ufg(6), < 1/6 1/5 1/4 1/3 2/5 1/2 3/5 2/3 3/4 4/5 5/6 1/1 6/5 5/4 4/3 3/2 5/3 2/1 5/2 3/1 4/1 5/1 6/1 >;
is ufg(5), < 1/5 1/4 1/3 2/5 1/2 3/5 2/3 3/4 4/5 1/1 5/4 4/3 3/2 5/3 2/1 5/2 3/1 4/1 5/1 >;

sub ufg($n)
{
    (1..$n X/ 1..$n).unique
                    .sort
                    .map(*.nude.join('/'))
}
