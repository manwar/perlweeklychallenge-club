#!/usr/bin/env raku

use Test;
use task-two;

my @testcases = (
  {input => (1,1,2,2,2,3),   output=> (3,1,1,2,2,2)},
  {input => (2,3,1,2,3), output=> (1,3,3,2,2)},
  {input => (222,33,1,222,33), output=> (1,222,222,33,33)},
);

plan @testcases.elems;
is-deeply solution($_<input>), $_<output> for @testcases;