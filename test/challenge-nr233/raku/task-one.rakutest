#!/usr/bin/env raku

use Test;
use task-one;

my @testcases = (
  {input => ("aba", "aabb", "abcd", "bac", "aabc"),   output=> 2},
  {input => ("aabb", "ab", "ba"), output=> 3},
  {input => ("AAbb", "ab", "ba"), output=> 1},
);

plan @testcases.elems;
is solution($_<input>), $_<output> for @testcases;
